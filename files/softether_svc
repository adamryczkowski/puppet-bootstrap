[Unit]
    Description=SoftEther VPN Client
    After=network.target auditd.service
    
[Service]
    Type=forking
    ExecStart=/bin/bash /usr/local/lib/softether/start_vpn.sh
    ExecStop=/usr/bin/vpnclient stop
    KillMode=process
    Restart=on-failure
    
[Install]
    WantedBy=multi-user.target
